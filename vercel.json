{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "cd client && npm install && npm run build",
    "outputDirectory": "client/dist",
    "framework": null,
    "installCommand": "npm install --prefix client",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}